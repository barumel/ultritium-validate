    "test": "mocha --recursive --bail",
    "test:coverage": "nwb test --coverage",
    "test:watch": "nwb test --server"
  },
  "keywords": [
    "ultritium",
    "validate"
  ],
  "dependencies": {
    "lodash": "^4.17.15",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "assertthat": "^0.10.3",
    "eslint": "^4.7.2",
    "eslint-config-es": "^0.8.12",
    "mocha": "^3.5.3",
    "nodemon": "^1.19.0",
    "nwb": "^0.23.0"
  },
  "author": "barumel",
  "homepage": "",
  "license": "MIT",
  "repository": "barumel/ultritium-validate"
}
