{
  "name": "@ultritium/validate",
  "version": "0.4.0",
  "description": "Ultritium validator",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-web-module --no-demo",
    "start": "nodemon -w src -x 'nwb build-web-module --no-demo --copy-files'",
