{
  "name": "@ultritium/validate",
  "version": "1.3.0",
  "description": "Ultritium validator",
  "type": "module",
  "browser": "./dist/bundle.esm.js",
  "module": "./dist/bundle.cjs.js",
  "main": "./dist/bundle.cjs.js",
  "exports": {
    ".": {
      "import": "./dist/bundle.esm.js",
      "require": "./dist/bundle.cjs.js"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./build.prod.cjs",
    "start": "node ./run-dev.cjs",
    "test": "mocha --recursive --bail"
  },
  "keywords": [
    "ultritium",
    "validate"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "assertthat": "^0.10.3",
    "esbuild": "^0.23.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "mocha": "^10.7.3"
  },
  "author": "barumel",
  "homepage": "",
  "license": "MIT",
  "repository": "barumel/ultritium-validate"
}
